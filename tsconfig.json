{
  "compilerOptions": {
    "typeRoots": [
      "node_modules/@types"
    ],
    "moduleResolution": "node",
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "Config/*": ["config/*"],
      "Views/*": ["src/views/*"],
      "Utils/*": ["src/utils/*"],
      "Layouts/*": ["src/layouts/*"],
      "Assets/*": ["assets/*"],
      "Redux/*": ["src/redux/*"],
      "Types/*": ["src/types/*"]
    }
  }
}
