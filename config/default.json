{
  "default": "default_config",
  "graphqlEndpoint": "https://graphql-pokemon.now.sh/",
  "port": 3011,
  "i18n": {
    "rootPath": "/src/i18n",
    "fallbackPath": "/dist",
    "config": {
      "fallbackLng": "es_CL",
      "keySeparator": ":",
      "nsSeparator": "."
    },
    "languages": [
      {
        "name": "Chile",
        "domain": ".cl",
        "code": "cl",
        "locale": "es_CL",
        "default": true
      },
      {
        "name": "Brasil",
        "domain": ".com.br",
        "code": "br",
        "locale": "pt_BR",
        "default": false
      }
    ]
  }
}
