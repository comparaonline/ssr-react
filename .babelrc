{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import",
      "universal-import",
      ["babel-plugin-styled-components", { "ssr": true }]
    ],
    "env": {
      "development": {
        "plugins": ["react-hot-loader/babel"]
      },
      "production": {
        "plugins": []
      }
    }
}
