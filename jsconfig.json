{
  "compilerOptions": {
    "target": "ES6",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "module": "commonjs",
    "paths": {
      "Config/*": ["config/*"],
      "Views/*": ["src/views/*"],
      "Utils/*": ["src/utils/*"],
      "Layouts/*": ["src/layouts/*"],
      "Assets/*": ["assets/*"],
      "Redux/*": ["src/redux/*"],
    }
  },
  "include": ["**/*.js"],
  "exclude": ["dist"]
}
