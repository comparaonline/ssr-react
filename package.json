{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "ssr for react",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "build:client:dev": "cross-env NODE_ENV=development webpack --config webpack/client/index.js",
    "build:client:prod": "cross-env NODE_ENV=production webpack -p --config webpack/client/index.js",
    "build:server:dev": "cross-env NODE_ENV=development webpack --config webpack/server/index.js",
    "build:server:prod": "cross-env NODE_ENV=production webpack -p --config webpack/server/index.js",
    "start:spa:server": "http-server-spa dist",
    "start:dev": "npm run clean && cross-env NODE_ENV=development babel-node src/server/index.js",
    "start:prod": "npm run clean && cross-env NODE_ENV=production babel-node src/server/index.js"
  },
  "keywords": [
    "react",
    "ssr",
    "graphql"
  ],
  "author": "",
  "license": "ISC",
