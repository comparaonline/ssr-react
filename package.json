{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "ssr for react",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test:jest": "jest",
    "test:jest:update": "jest --updateSnapshot",
    "lint:js": "eslint --ext .js src",
    "clean": "rimraf dist",
    "build:client:dev": "cross-env NODE_ENV=development webpack --config webpack/client/index.js",
    "build:client:prod": "cross-env NODE_ENV=production webpack -p --config webpack/client/index.js",
    "build:router:dev": "cross-env NODE_ENV=development webpack --config webpack/router/index.js",
    "build:router:prod": "cross-env NODE_ENV=production webpack -p --config webpack/router/index.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/server/index.js",
    "build": "npm run clean && cross-env NODE_ENV=production ONLY_BUILD=true babel-node src/server/index.js",
    "start:spa:server": "http-server-spa dist",
    "start:dev": "npm run clean && cross-env NODE_ENV=development babel-node src/server/index.js",
    "start:prod": "npm run clean && cross-env NODE_ENV=production babel-node src/server/index.js",
    "analyze:dev": "npm run clean && cross-env NODE_ENV=development ANALYZE=true babel-node src/server/index.js",
    "analyze:prod": "npm run clean && cross-env NODE_ENV=production ANALYZE=true babel-node src/server/index.js"
  },
  "keywords": [
    "react",
    "ssr",
    "graphql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "apollo-cache-inmemory": "^1.2.2",
    "apollo-client": "^2.3.2",
    "apollo-link-http": "^1.5.4",
    "app-root-path": "^2.1.0",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "history": "^4.7.2",
    "i18next": "^11.3.2",
    "i18next-express-middleware": "^1.1.1",
    "i18next-node-fs-backend": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.4.0",
    "react-i18next": "^7.7.0",
    "react-redux": "^5.0.7",
    "react-safety-helmet": "^6.2.1",
    "react-universal-component": "^3.0.0",
    "redux": "^4.0.0",
    "styled-components": "^3.3.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-flush-chunks": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-plugin-universal-import": "^3.1.2",
    "colors": "^1.3.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.1.6",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-alias": "^1.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "extract-css-chunks-webpack-plugin": "^3.0.10",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-server-spa": "^1.2.0",
    "image-webpack-loader": "^4.3.0",
    "jest": "^23.1.0",
    "jest-css-modules": "^1.1.0",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.4.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "typescript": "^3.1.6",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-merge": "^4.1.2",
    "webpack-stats-plugin": "^0.2.1",
    "write-file-webpack-plugin": "^4.3.2"
  }
}
